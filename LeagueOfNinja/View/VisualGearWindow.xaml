<Window x:Class="LeagueOfNinja.View.VisualGearWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LeagueOfNinja.View"
        mc:Ignorable="d"
        Title="VisualGearWindow" Height="300" Width="250">

    <Window.Background>
        <ImageBrush ImageSource="Images/NinjaPicca.png" />
    </Window.Background>
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="43" Margin="30,65,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="44"/>
        <TextBox x:Name="textBox_Copy" HorizontalAlignment="Left" Height="43" Margin="188,119,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="44"/>
        <TextBox x:Name="textBox_Copy1" HorizontalAlignment="Left" Height="43" Margin="162,10,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="44"/>
        <TextBox x:Name="textBox_Copy2" HorizontalAlignment="Left" Height="43" Margin="10,184,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="44"/>
        <TextBox x:Name="textBox_Copy3" HorizontalAlignment="Left" Height="43" Margin="150,213,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="44"/>

    </Grid>
</Window>
